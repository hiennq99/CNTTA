#VRML V2.0 utf8

Transform {
	children Collision {
		children [
			DEF	cam_bien ProximitySensor {
				center 0  0  0
				size 10 15 20
	    	}
			Sound {
				source DEF chao_mung AudioClip {
					loop FALSE
					url	["..\audio\chao_mung_1.wav"]
				}
				location 0 0 0
				minBack	0
				minFront 0
				maxBack	20
				maxFront 20
			}
		]
	}
	ROUTE cam_bien.enterTime      TO chao_mung.startTime
	ROUTE cam_bien.exitTime       TO chao_mung.stopTime
}

