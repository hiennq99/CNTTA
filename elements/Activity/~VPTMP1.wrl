DEF	ma Script{
eventIn	SFBool turnOn
eventOut SFBool	On
eventOut SFBool	Off
field SFBool turn FALSE
url	"javascript:
function turnOn(val){
if(val){
turn = !turn;
if(turn){
On= TRUE;
Off= FALSE;
}
else
{
On= FALSE;
Off= TRUE;
}
}
}
"
}
DEF	touch1 TouchSensor{
enabled	TRUE
}
DEF	time1 TimeSensor{
cycleInterval 1
enabled	FALSE
}
DEF	orien1 OrientationInterpolator{
key[0 1]
keyValue[1 0 0 0,1 0 0 -1.04]
}
DEF	pos1 PositionInterpolator{
key[0 1]
keyValue[0 0 0,0 -2 -2]
}
ROUTE touch1.isActive TO ma.turnOn
ROUTE ma.On	TO time1.enabled
ROUTE ma.Off TO	time1.enabled
ROUTE touch1.touchTime TO time1.startTime
ROUTE time1.fraction_changed TO	orien1.set_fraction
ROUTE orien1.value_changed TO FootLeft.rotation
ROUTE orien1.value_changed TO FootRight.rotation
ROUTE time1.fraction_changed TO	pos1.set_fraction
ROUTE pos1.value_changed TO	nguoi.translation


