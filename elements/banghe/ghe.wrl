#VRML V2.0 utf8

#Mặt ghế
Transform {
	children [
		Shape {
			geometry Box {
				size .5 .01 .5
			}
			appearance Appearance {
				texture	ImageTexture {
					url	["images\Go1.jpg"]
				}
			}
		}
	]
}
#chân ghế
DEF hang1 Transform {
	children [
		DEF	chan1 Transform	{
			translation	-.20 -.25 -.20
			children [
				Shape {
					geometry Cylinder {
						height .5
						radius .02
					}
					appearance Appearance {
						texture	ImageTexture {
							url	["images\Go1.jpg"]
						}
					}
				}
			]
		}
		Transform {
			translation	0 0 .4
			children [
				USE	chan1 
			]
		}
	]
}
Transform {
	translation	.4 0 0
	children [
		USE	hang1
	]
}

