#VRML V2.0 utf8
#Mặt trước
EXTERNPROTO	Wall-1 [field SFVec3f size] "../../_proto.wrl#Wall-1"
EXTERNPROTO	Wall-2 [field SFVec3f size] "../../_proto.wrl#Wall-2"
EXTERNPROTO	Wall-3 [field SFVec3f size] "../../_proto.wrl#Wall-3"
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size, field SFFloat trans] "../../_proto.wrl#Wall"

Viewpoint {
	position 0 0 20
}
DEF NuaMatTruoc Group {
	children [
		# Tường phía trên
		DEF TuongTren Transform	{
			translation	5 3.35 0
			children Wall-1 { size 10 1.3 0.3 }
		}
		# Cột tường
		DEF	CotTuong Transform {
			translation	1.4 1.35 0
			children Wall-1 { size 0.4 2.7 0.3 }
		}
		Transform {
			translation	2.8 0 0
			children USE CotTuong
		}
		Transform {
			translation	5.6 0 0
			children USE CotTuong
		}
		Transform {
			translation	8.4 0 0
			children USE CotTuong
		}
		# Cột ngang
		DEF	CotNgang Transform {
			translation	2.8 0.5 0
			children Wall-1 { size 2.4 1 0.3 }
		}
		Transform {
			translation	5.6 0 0
			children USE CotNgang
		}
	]
}
Transform {
	rotation 0 1 0 3.14159
	children USE NuaMatTruoc
}
#Cửa ra vào
Inline { url "cua_ra_vao.wrl" }
Transform {
	translation	5.6 0 0
	children Inline { url "cua_ra_vao_2.wrl" }
}
Transform {
	translation	-5.6 0 0
	children Inline { url "cua_ra_vao_3.wrl" }
}
#Cửa sổ
Inline { url "cua_so.wrl" }
Transform {
	translation	5.6 0 0
	children Inline { url "cua_so.wrl" }
}
Transform {
	translation	-5.6 0 0
	children Inline { url "cua_so.wrl" }
}
Transform {
	translation	-11.2 0 0
	children Inline { url "cua_so.wrl" }
}
Transform {
	children Inline	{ url "person/person_2.wrl" }
}
Transform {
	children Inline	{ url "person/person_3.wrl" }
}
Transform {
	children Inline	{ url "person/person_4.wrl" }
}