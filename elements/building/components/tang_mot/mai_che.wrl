#Tấm thanh nối 
DEF ThanhNoi Transform {
	translation	0 3.5 0.5
	rotation 1 0 0 -1
	children [
		DEF ThanhNoiNho Shape {
			appearance Appearance {
				material Material {
					diffuseColor 0 0 0
					specularColor .29 .3 .29
					ambientIntensity 0
					shininess .08
				}
			}
			geometry Cylinder {
				height 1.5
				radius 0.01
			}
		}
		Transform {
			translation	1.875 0 0
			children USE ThanhNoiNho
		}
		Transform {
			translation	3.75 0 0
			children USE ThanhNoiNho
		}
		Transform {
			translation	5.625 0 0
			children USE ThanhNoiNho
		}
		Transform {
			translation	7.5 0 0
			children USE ThanhNoiNho
		}
	]
}
Transform {
	translation	-7.5 0 0 
	children USE ThanhNoi
}
