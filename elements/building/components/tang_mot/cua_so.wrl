#VRML V2.0 utf8
#Cửa sổ
EXTERNPROTO	Wall-1 [field SFVec3f size] "../../_proto.wrl#Wall-1"
EXTERNPROTO	Wall-2 [field SFVec3f size] "../../_proto.wrl#Wall-2"
EXTERNPROTO	Wall-3 [field SFVec3f size] "../../_proto.wrl#Wall-3"
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size, field SFFloat trans] "../../_proto.wrl#Wall"

#Thanh ngang trong suốt phía trên
Transform {
	translation	2.8 2.5 0
	children Wall { size 2.4 0.4 0.2 trans 0.5 }
}
#Thanh dọc trong suốt
DEF ThanhDoc Transform {
	translation	3.8 1.35 0
	children Wall { size 0.4 2.7 0.2 trans 0.5 }
}
Transform {
	translation	-2 0 0
	children USE ThanhDoc
}
#Thanh trang trí ngang
DEF ThanhTTNgang Transform {
	translation	2.8 2.7 0.15
	children Wall { size 2.4 0.05 0.05 color 1 1 1 }
}
Transform {
	translation	0 -0.4 0
	children USE ThanhTTNgang
}
#Thanh trang trí dọc
DEF	ThanhTTDoc Transform {
	translation	1.6 1.85 0.15
	children Wall { size 0.05 1.7 0.05 color 1 1 1}
}
Transform {
	translation	0.4 0 0
	children USE ThanhTTDoc
}
Transform {
	translation	2.4 0 0
	children USE ThanhTTDoc
}
Transform {
	translation	2 0 0
	children USE ThanhTTDoc
}
#Thanh nhỏ nằm dọc
Transform {
	translation	2.8 2.5 0.15
	children Wall { size 0.05 0.4 0.05 color 1 1 1 }
}
#Thanh nhỏ nằm ngang
DEF ThanhTTNgangNho Transform {
	translation	1.8 1.025 0.15
	children Wall { size 0.4 0.05 0.05 color 1 1 1}
}
Transform {
	translation	2 0 0
	children USE ThanhTTNgangNho
}
DEF	CanhCuaTrai Transform {
	translation	2.4 1.625 0
	children [
		Wall { size 0.8 1.3 0 trans 0.5 }
		Transform {
			translation	0 0.625 0.15
			children Wall { size 0.8 0.05 0.05 color 1 1 1}
		}
		Transform {
			translation	0 -0.6 0.15
			children Wall { size 0.8 0.05 0.05 color 1 1 1}
		}
		Transform {
			translation	-0.375 0 0.15
			children Wall { size 0.05 1.2 0.05 color 1 1 1}
		}
		Transform {
			translation	0.375 0 0.15
			children Wall { size 0.05 1.2 0.05 color 1 1 1}
		}
		DEF	actionKeoCuaTrai PlaneSensor {
			enabled	TRUE
			offset 2.4 1.625 0
			autoOffset TRUE
			maxPosition	3.2 1.625
			minPosition	2.4 1.625
		}
	]
}
DEF	CanhCuaPhai Transform {
	translation	3.2 1.625 0.05
	children [
		Wall { size 0.8 1.3 0 trans 0.5 }
		Transform {
			translation	0 0.625 0.15
			children Wall { size 0.8 0.05 0.05 color 1 1 1 }
		}
		Transform {
			translation	0 -0.6 0.15
			children Wall { size 0.8 0.05 0.05 color 1 1 1 }
		}
		Transform {
			translation	-0.375 0 0.15
			children Wall { size 0.05 1.2 0.05 color 1 1 1 }
		}
		Transform {
			translation	0.375 0 0.15
			children Wall { size 0.05 1.2 0.05 color 1 1 1 }
		}
		DEF	actionKeoCuaPhai PlaneSensor {
			enabled	TRUE
			offset 3.2 1.625 0.05
			autoOffset TRUE
			maxPosition	3.2 1.625
			minPosition	2.4 1.625
		}
	]
}
ROUTE actionKeoCuaTrai.translation_changed TO CanhCuaTrai.translation
ROUTE actionKeoCuaPhai.translation_changed TO CanhCuaPhai.translation