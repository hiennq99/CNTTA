#VRML V2.0 utf8

# Bậc cầu thang
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size ] "_proto.wrl"
Group {
	children [
		DEF	transBacThang Transform	{
			children Wall { size 14 0.2 0.4 }
		}
		Transform {
			translation	0 -0.2 0.4
			children USE transBacThang	
		}
		Transform {
			translation	0 -0.4 0.8
			children USE transBacThang	
		}
		Transform {
			translation	0 -0.6 1.2
			children USE transBacThang	
		}
		Transform {
			translation	0 -0.8 1.6
			children USE transBacThang	
		}
	]
}