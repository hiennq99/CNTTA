#VRML V2.0 utf8

# Căn nhà ở giữa
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size ] "_proto.wrl"
# Sàn
Transform {
	translation	-5 -1.2 -6.4
	children [
		Wall { size 2 0.03 11 color 1 1 1 }
	]
}
# Tấm sau lưng
Transform {
	translation	-5 0.1 -11.98
	children Wall { size 2 2.7 0.03 }
}
# Bệ lan can trước mặt
Transform {
	translation	-5 -1 -1
	children Wall { size 2 0.5 0.5 color 1 1 1 }
}
DEF grLanCan Group {
	children [
		DEF ThanhLanCan Transform {
			translation	-5 -0.5 -1
			children [
				Wall { size 0.02 0.5 0.02 color 0 0 0 }
			]
		}
		Transform {
			translation	-0.1 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.2 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.3 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.4 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.5 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.6 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.7 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.8 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-0.9 0 0
			children USE ThanhLanCan
		}
		Transform {
			translation	-1 0 0
			children USE ThanhLanCan
		}
	]
}
Transform {
	translation	1 0 0
	children USE grLanCan
}
Transform {
	translation	-5 -0.23 -1
	children Wall { size 2 0.05 0.05 color 0 0 0 } 
}