#VRML V2.0 utf8
#Ô cửa ra vào
EXTERNPROTO	Wall-1 [field SFVec3f size] "../../_proto.wrl#Wall-1"
EXTERNPROTO	Wall-2 [field SFVec3f size] "../../_proto.wrl#Wall-2"
EXTERNPROTO	Wall-3 [field SFVec3f size] "../../_proto.wrl#Wall-3"
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size, field SFFloat trans] "../../_proto.wrl#Wall"

Transform {
	children [
		#Tấm tường đằng sau
		Transform {
			translation	0 0 0
			children Wall-1 { size 3.4 3 0.1 }
		}
		#Tấm 2 bên
		Transform {
			translation	1.2 0 0.63
			children Wall { size 0.1 3 1 color 0.523 0.556 0.588 }
		}
		Transform {
			translation	-1.2 0 0.63
			children Wall { size 0.1 3 1 color 0.523 0.556 0.588 }
		}
		#Cửa chính
		Transform {
			translation	-0.5 -0.2 0.1
			children Wall { size 1 2.5 0.05 color 1 1 1 }
		}
		#Cửa chính - tấm nền đen
		Transform {
			translation	-0.5 0.3 0.15
			children Wall { size 0.8 1.2 0.01 trans 0.5 }
		}
		#Cửa chính - tay nắm
		Transform {
			translation	-0.1 -0.5 0.15
			children Wall { size 0.05 0.05 0.01 trans 0.5 }
		}
		#Cửa sổ
		#Cửa sổ - nền đen
		Transform {
			translation 0.7 0.4 0.1
			children Wall { size 1.2 1.3 0.1 trans 0.5 }
		}
		#Cửa sổ - viền ngang
		Transform {
			translation 0.7 1 0.15
			children Wall { size 1.2 0.05 0.05 color 1 1 1 }
		}
		Transform {
			translation 0.7 0.8 0.15
			children Wall { size 1.2 0.05 0.05 color 1 1 1 }
		}
		Transform {
			translation 0.7 -0.3 0.15
			children Wall { size 1.2 0.05 0.05 color 1 1 1 }
		}
		#Cửa sổ - viền dọc
		Transform {
			translation 0.6 0.35 0.15
			children Wall { size 0.05 1.3 0.05 color 1 1 1 }
		}
		Transform {
			translation 0.1 0.35 0.15
			children Wall { size 0.05 1.3 0.05 color 1 1 1 }
		}
		Transform {
			translation 1.15 0.35 0.15
			children Wall { size 0.05 1.3 0.05 color 1 1 1 }
		}
	]
}