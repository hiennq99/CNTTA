#VRML V2.0 utf8
#Mặt trước
EXTERNPROTO	Wall-1 [field SFVec3f size] "../../_proto.wrl#Wall-1"
EXTERNPROTO	Wall-2 [field SFVec3f size] "../../_proto.wrl#Wall-2"
EXTERNPROTO	Wall-3 [field SFVec3f size] "../../_proto.wrl#Wall-3"
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size, field SFFloat trans] "../../_proto.wrl#Wall"

Viewpoint {
	position 0 0 20
}
Group {
	children [
		#Tường ngang mỏng
		DEF	TuongNgangMong Transform {
			translation	5.6 0 0
			children Wall-1 { size 8.8 0.4 0.3 }
		}
		Transform {
			translation	0 2.6 0
			children USE TuongNgangMong
		}
		#Tường dọc
		DEF	TuongDoc Transform {
			translation	1.7 1.3 0
			children Wall-1 { size 1 3 0.3 }
		}
		Transform {
			translation	2.2 0 0
			children USE TuongDoc
		}
		Transform {
			translation	5.6 0 0
			children USE TuongDoc
		}
		Transform {
			translation	7.8 0 0
			children USE TuongDoc
		}
		#Tường ngang dày
		DEF	TuongNgangDay Transform	{
			translation	2.8 0.4 0
			children Wall-1 { size 2 1.2 0.3 }
		}
		Transform {
			translation	5.6 0 0
			children USE TuongNgangDay
		}
		#Viền cửa sổ
		DEF	VienCuaSo Group	{
			children [
				#Viền ngang trên
				DEF VienNgang Transform {
					translation 2.8 2.35 0.2
					children Wall { size 1.4 0.1 0.1 color 0.523 0.556 0.588 }
				}
				Transform {
					translation 0 -1.4 0
					children USE VienNgang
				}
				#Viền ngang trong
				DEF VienNgangTrong Transform {
					translation 2.8 2.3 0.15
					children Wall { size 1.4 0.1 0.1 color 1 1 1 }
				}
				Transform {
					translation 0 -1.3 0
					children USE VienNgangTrong
				}
				Transform {
					translation 0 -0.3 0
					children USE VienNgangTrong
				}
				#Viền dọc
				DEF VienDoc Transform {
					translation 2.15 1.7 0.2
					children Wall { size 0.1 1.4 0.1 color 0.523 0.556 0.588 }
				}
				Transform {
					translation 1.3 0 0
					children USE VienDoc
				}
				#Viền dọc trong
				DEF VienDocTrong Transform {
					translation 2.2 1.7 0.15
					children Wall { size 0.1 1.4 0.1 color 1 1 1 }
				}
				Transform {
					translation 1.2 0 0
					children USE VienDocTrong
				}
				#Dọc nhỏ
				Transform {
					translation	2.8 2.12 0.15
					children Wall { size 0.05 0.3 0.1 color 1 1 1}
				}
			]
		}
		Transform {
			translation	5.6 0 0
			children USE VienCuaSo
		}
		Transform {
			translation	5.6 -0.2 0.18
			children Inline	{ url "lan_can.wrl" }
		}
	]
}