#VRML V2.0 utf8

EXTERNPROTO	Wall [field SFColor color, field SFVec3f size ] "_proto.wrl"
# Mặt trước
# Tường trên
Transform {
	translation	0 1.35 0
	children Wall { size 8 0.2 0.3 }
}

# Tường nhỏ trên
Transform {
	translation	-2.8 1.2 0
	children Wall { size 2 0.1 0.3 }
}
Transform {
	translation	2.8 1.2 0
	children Wall { size 2 0.1 0.3 }
}

# Tường nhỏ dưới
Transform {
	translation	-2.8 -0.75 0
	children Wall { size 2 1 0.3 } 
}

Transform {
	translation	2.8 -0.75 0
	children Wall { size 2 1 0.3 } 
}

# Tường 2 bên
Transform {
	translation	2 0 0
	children Wall { size 0.6 2.5 0.3 } 
}
Transform {
	translation	-2 0 0
	children Wall { size 0.6 2.5 0.3 } 
}

Transform {
	translation	-3.7 0 0
	children Wall { size 0.6 2.5 0.3 }
}

Transform {
	translation	3.7 0 0
	children Wall { size 0.6 2.5 0.3 } 
}

# Khung cửa sổ
DEF KhungCuaSo Transform {
	translation	-2.85 1.2 0.2
	children [
		Wall { size 1.3 0.1 0.1 color 1 1 1 }
		Transform {
			translation	0 -1.5 0
			children Wall { size 1.3 0.1 0.1 color 1 1 1 }
		}
		Transform {
			translation	-0.6 -0.8 0
			children Wall { size 0.1 1.5 0.1 color 1 1 1 }
		}
		Transform {
			translation	0.6 -0.8 0
			children Wall { size 0.1 1.5 0.1 color 1 1 1 } 
		}
	]
}
Transform {
	translation	5.7 0 0
	children USE KhungCuaSo
}


