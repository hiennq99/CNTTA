#VRML V2.0 utf8
#Mặt trước
EXTERNPROTO	Wall-1 [field SFVec3f size] "../../_proto.wrl#Wall-1"
EXTERNPROTO	Wall-2 [field SFVec3f size] "../../_proto.wrl#Wall-2"
EXTERNPROTO	Wall-3 [field SFVec3f size] "../../_proto.wrl#Wall-3"
EXTERNPROTO	Wall [field SFColor color, field SFVec3f size, field SFFloat trans] "../../_proto.wrl#Wall"

Viewpoint {
	position 0 0 20
}
Group {
	children [
		#Tường ngang mỏng
		DEF	TuongNgangMong Transform {
			translation	5.6 0 0
			children Wall-1 { size 8.8 0.4 0.3 }
		}
		Transform {
			translation	0 2.6 0
			children USE TuongNgangMong
		}
		#Tường dọc
		DEF	TuongDoc Transform {
			translation	1.7 1.3 0
			children Wall-1 { size 1 3 0.3 }
		}
		Transform {
			translation	2.2 0 0
			children USE TuongDoc
		}
		Transform {
			translation	5.6 0 0
			children USE TuongDoc
		}
		Transform {
			translation	7.8 0 0
			children USE TuongDoc
		}
		#Tường ngang dày
		DEF	TuongNgangDay Transform	{
			translation	2.8 0.4 0
			children Wall-1 { size 2 1.2 0.3 }
		}
		Transform {
			translation	5.6 0 0
			children USE TuongNgangDay
		}
		#Viền cửa sổ
		Inline { url "cua_so.wrl" }
		Transform {
			translation	5.6 0 0
			children Inline { url "cua_so.wrl" }
		}
		Transform {
			translation	5.6 -0.2 0.18
			children Inline	{ url "lan_can.wrl" }
		}
		#Cửa ra vào
		Transform {
			translation	5.6 1.3 -1
			children Inline	{ url "cua_ra_vao.wrl" }
		}
	]
}
