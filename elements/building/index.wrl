#VRML V2.0 utf8

EXTERNPROTO	Wall [field SFVec3f size, field SFColor color] "_proto.wrl#Wall"
Viewpoint {
	position 0 10 40
}

DEF	TOA_NHA Group {
	children [
		Transform {
			translation	0 -0.5 1.1
			children Inline { url "components/bac_thang/_index.wrl" }
		}
		Transform {
			translation	0 0 0
			children Inline	{ url "components/tang_mot/_index.wrl" }
		}
		# Tầng 2
		Transform {
			translation	0 4 0
			children Inline	{ url "components/tang_tren/_index.wrl" }
		}
		Transform {
			translation	0 7 0
			children Inline	{ url "components/tang_tren/_index.wrl" }
		}
		Transform {
			translation	0 10 0
			children Inline	{ url "components/tang_tren/_index.wrl" }
		}
		Transform {
			translation	0 13 0
			children Inline	{ url "components/tang_tren/_index.wrl" }
		}
		Transform {
			translation	0 16 0
			children Inline	{ url "components/tang_tren/_index.wrl" }
		}
		Transform {
			translation	0 0 0 
			children Inline	{ url "components/tang_thuong/_index.wrl" }
		}
		#Biển số nhà
		Transform {
			translation	-2.8 10.2 0.2
			children [
				Wall { size 1 0.5 0.01 color 1 1 1 }
				Transform {
					translation	0 -0.15 0.01
					children [
						Shape {
							appearance Appearance {
								material Material {
									diffuseColor 0.172 0.2 0.411
								}
							}
							geometry Text {
								string ["CT6-B"]
								fontStyle FontStyle	{
									family "Open Sans"
									topToBottom	TRUE
									size 0.6
									justify	"MIDDLE"
									spacing	1
									style "BOLD"
								}
								length [1]
								maxExtent 10
							}
						}
					]
				}
			]
		}
	]	
}