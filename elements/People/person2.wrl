#VRML V2.0 utf8
DEF	dichuyen Transform {children [
		DEF	time TimeSensor	{
			cycleInterval 60
			loop TRUE
		}
		DEF	di PositionInterpolator {
			key	[0,.25, .5, .75, 1]
			keyValue [-10 0 0,
				17 0 0,
				17 0 -33,
	    		-17 0 -33
				-17 0 0]
		}
		DEF	quay OrientationInterpolator {
			key	[0,.3,.6,.9,1]
			keyValue [0 1 0 -1.57,
				0 0 0 0, 
				0 1 0 -1.57,
				0 1 0 -1.57,
	    		0 1 0 0] 
		}
		DEF	nguoi Transform {
			scale .2 .2 .2
			translation	0 0 0
			children [Inline {
					url	"person.wrl"
				}]
		}
	]}

ROUTE time.fraction_changed	TO di.set_fraction
ROUTE di.value_changed TO	nguoi.translation
ROUTE time.fraction_changed	TO quay.set_fraction
ROUTE quay.value_changed TO	nguoi.rotation